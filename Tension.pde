void juego() {
  background(#000000);
  
//jugador 1:
  if (j1==pv1) {
    seleccionado=0;
    pv1.jugando();
  }
  if (j1==pv2) {
    seleccionado=1;
    pv2.jugando();
  }
  if (j1==pv3) {
    seleccionado=2;
    pv3.jugando();
  }
  if (j1==pv4) {
    seleccionado=3;
    pv4.jugando();
  }
  if (j1==pv5) {
    seleccionado=4;
    pv5.jugando();
  }
  
